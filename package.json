    "next": "10.0.3",
    "next-compose-plugins": "^2.2.1",
    "nprogress": "^0.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "@types/node": "^14.14.13",
    "@types/nprogress": "^0.2.0",
