{
  "name": "@avila-tek/next-template",
  "version": "1.7.0",
  "description": "A next.js template for starting frontend projects in Avila Tek",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "lint": "eslint . --fix",
    "git:add": "git add .",
    "precommit": "run-s lint git:add",
    "prepush": "run-s lint git:add",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush"
    }
  },
  "dependencies": {
    "@sentry/integrations": "^6.2.2",
    "@sentry/node": "^6.2.2",
    "@sentry/react": "^6.2.2",
    "@sentry/tracing": "^6.2.2",
    "@sentry/webpack-plugin": "^1.14.2",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "avilatek-utils": "^0.1.0",
    "framer-motion": "^3.10.2",
    "nanoid": "^3.1.20",
    "next": "10.0.8",
    "next-compose-plugins": "^2.2.1",
    "nprogress": "^0.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "@types/node": "^14.14.33",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-avilatek": "^1.5.0",
    "eslint-config-avilatek-typescript": "^1.5.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.2.3"
  },
  "volta": {
    "node": "14.16.0",
    "npm": "7.6.2"
  }
}
