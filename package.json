{
  "name": "avila-tek-next-template",
  "version": "1.6.0",
  "description": "A next.js template for starting frontend projects in Avila Tek",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7777",
    "build": "next build",
    "start": "next start",
